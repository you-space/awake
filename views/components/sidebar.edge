@set('links', [
    {
        label: 'Home',
        href: '/'
    },
    {
        label: 'Youtube videos',
        href: '/youtube'
    },
    {
        label: 'Local videos',
        href: '/local'
    },
])

<ul class="py-5 px-8 w-full">
